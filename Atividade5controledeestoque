public class ControleEstoque {
    public static void main(String[] args) {
        String nomeSmartphone = "Xiaomi-13-lite";
        String marcaSmartphone = "Xiaomi";
        double precoSmartphone = 1.999;
        int estoqueSmartphone = 20;
        
        String nomeLaptop = "Notebook Inspiron 15";
        String marcaLaptop = "Dell";
        double precoLaptop = 2599;
        int estoqueLaptop = 12;
        
        String nomeFone = "Sony WH-1000XM4";
        String marcaFone = "Sony";
        double precoFone = 838;
        int estoqueFone = 5;
        
        System.out.println("Dados do Smartphone:");
        System.out.println("Nome: " + nomeSmartphone);
        System.out.println("Marca: " + marcaSmartphone);
        System.out.println("Preco R$: " + precoSmartphone);
        System.out.println("Quantidade em estoque: " + estoqueSmartphone);
        System.out.println("Dados do Laptop:");
        System.out.println("Nome: " + nomeLaptop);
        System.out.println("Marca: " + marcaLaptop);
        System.out.println("Preco R$: " + precoLaptop);
        System.out.println("Quantidade em estoque: " + estoqueLaptop);
        System.out.println("Dados do fone de ouvido bluetooth: ");
        System.out.println("Nome: " + nomeFone);
        System.out.println("Marca: " + marcaFone);
        System.out.println("Preco R$: " + precoFone);
        System.out.println("Quantidade em estoque: " + estoqueFone);
        
        
    }

}
